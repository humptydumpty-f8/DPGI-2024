<Window x:Class="_70_Інтерфейс_ICommand.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_70_Інтерфейс_ICommand"
        xmlns:custom="clr-namespace:_70_Інтерфейс_ICommand"
        mc:Ignorable="d"
        Title="Lection 7-0 (RoutedCommand)" Height="450" Width="800">

    <Window.CommandBindings>
        <CommandBinding Command="{x:Static custom:MainWindow.CustomRoutedCommand}"
                    Executed="ExecutedCustomCommand"
                    CanExecute="CanExecuteCustomCommand" />
    </Window.CommandBindings>
    
    <Grid>
        <StackPanel>
            <Button Command="{x:Static custom:MainWindow.CustomRoutedCommand}" >Custom Routed Command</Button>
        </StackPanel>
    </Grid>
</Window>
